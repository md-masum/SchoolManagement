    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title - School Management</title>

    <!-- Mobile Specific Metas
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/icon" href="~/WebsiteTemplate/img/wpf-favicon.png" />

    <!-- CSS
    ================================================== -->
    <!-- Bootstrap css file-->
    <link href="~/WebsiteTemplate/css/bootstrap.min.css" rel="stylesheet">
    <!-- Font awesome css file-->
    <link href="~/WebsiteTemplate/css/font-awesome.min.css" rel="stylesheet">
    <!-- Superslide css file-->
    <link rel="stylesheet" href="~/WebsiteTemplate/css/superslides.css">
    <!-- Slick slider css file -->
    <link href="~/WebsiteTemplate/css/slick.css" rel="stylesheet">
    <!-- Circle counter cdn css file -->
    <link rel='stylesheet prefetch' href='~/WebsiteTemplate/css/jquery.circliful.css'>
    <!-- smooth animate css file -->
    <link rel="stylesheet" href="~/WebsiteTemplate/css/animate.css">
    <!-- preloader -->
    <link rel="stylesheet" href="~/WebsiteTemplate/css/queryLoader.css" type="text/css" />
    <!-- gallery slider css -->
    <link type="text/css" media="all" rel="stylesheet" href="~/WebsiteTemplate/css/jquery.tosrus.all.css" />
    <!-- Default Theme css file -->
    <link id="switcher" href="~/WebsiteTemplate/css/themes/default-theme.css" rel="stylesheet">
    <!-- Main structure css file -->
    <link href="~/WebsiteTemplate/style.css" rel="stylesheet">

    <!-- Google fonts -->
    <link href='http://fonts.googleapis.com/css?family=Merriweather' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>

